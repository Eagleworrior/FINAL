CSS files

styles/layout.css

`css
:root {
  --bg: #0e1117;
  --panel: #151a22;
  --text: #e6edf3;
  --muted: #a9b2c3;
  --accent: #7bdcff;
  --accent-2: #ff7bdc;
  --success: #47e5a4;
  --danger: #ff7b8a;
  --shadow: 0 10px 30px rgba(0,0,0,0.35);
  --radius: 14px;
  --gap: 16px;
  --max: 960px;
  --font: ui-sans-serif, system-ui, -apple-system, "Segoe UI", Roboto, "Helvetica Neue", Arial, "Noto Sans", "Apple Color Emoji", "Segoe UI Emoji";
}

* { box-sizing: border-box; }
html, body { height: 100%; }
body {
  margin: 0; background: var(--bg); color: var(--text);
  font: 16px/1.6 var(--font); -webkit-font-smoothing: antialiased; text-rendering: optimizeLegibility;
}

.skip-link { position: absolute; left: -9999px; }
.skip-link:focus { left: 8px; top: 8px; background: #000; color: #fff; padding: 8px 12px; border-radius: 6px; }

.hero-banner {
  position: relative; padding: 64px 20px 40px;
  text-align: center; background: radial-gradient(1200px 500px at 50% -10%, rgba(123,220,255,0.2), transparent);
}
.logo { width: 120px; height: auto; filter: drop-shadow(0 6px 16px rgba(0,0,0,0.4)); }
.headline { font-size: 40px; margin: 16px 0 8px; letter-spacing: 1px; }
.subhead { color: var(--muted); margin: 0 auto; max-width: 720px; }

.main-nav {
  display: flex; gap: 12px; align-items: center; justify-content: space-between;
  padding: 12px 20px; background: rgba(15,18,26,0.7); backdrop-filter: blur(8px);
  border-top: 1px solid rgba(255,255,255,0.06); border-bottom: 1px solid rgba(255,255,255,0.06);
}
.main-nav ul { list-style: none; display: flex; gap: 12px; padding: 0; margin: 0; flex-wrap: wrap; }
.main-nav a { color: var(--text); text-decoration: none; padding: 8px 12px; border-radius: 8px; }
.main-nav a:hover { background: rgba(255,255,255,0.06); }
.nav-tools { display: flex; gap: 8px; }

.content-grid {
  display: grid; grid-template-columns: repeat(auto-fit, minmax(260px, 1fr));
  gap: 20px; padding: 24px; max-width: 1200px; margin: 0 auto;
}
.module {
  background: linear-gradient(180deg, rgba(255,255,255,0.06), rgba(255,255,255,0.02));
  border: 1px solid rgba(255,255,255,0.06); border-radius: var(--radius); padding: 20px;
  box-shadow: var(--shadow);
}
.module-title { margin: 0 0 8px; font-size: 22px; }
.module-copy { margin: 0 0 16px; color: var(--muted); }

.btn {
  display: inline-flex; align-items: center; justify-content: center;
  padding: 10px 14px; border-radius: 10px; cursor: pointer; border: none; text-decoration: none;
  font-weight: 600; transition: transform .08s ease, box-shadow .2s ease, background .2s ease;
}
.btn:hover { transform: translateY(-1px); }
.btn:active { transform: translateY(0); }
.btn.ceremonial { color: #0a0d13; background: linear-gradient(90deg, var(--accent), var(--accent-2)); }
.btn.ghost { background: transparent; border: 1px solid rgba(255,255,255,0.2); color: var(--text); }

.footer { padding: 24px; text-align: center; color: var(--muted); }
.footer.compact { padding: 16px; }
.footer-icons img { width: 24px; height: 24px; opacity: .8; margin: 0 6px; }

.page-header {
  padding: 24px; text-align: center; background: radial-gradient(800px 400px at 50% -10%, rgba(255,123,220,0.16), transparent);
}
.page-header .back { color: var(--accent); text-decoration: none; }
.lede { color: var(--muted); margin-top: 8px; }

.content-narrow { max-width: var(--max); margin: 20px auto; padding: 0 20px; }
.center { text-align: center; }

.card {
  background: var(--panel); border: 1px solid rgba(255,255,255,0.08);
  border-radius: var(--radius); padding: 20px; margin-bottom: 16px; box-shadow: var(--shadow);
}

.grid-2 { display: grid; grid-template-columns: repeat(2, 1fr); gap: 12px; }
.stat { background: rgba(255,255,255,0.04); border-radius: 10px; padding: 12px; display: flex; justify-content: space-between; }
.stat .label { color: var(--muted); }
.stat .value.online { color: var(--success); }
.stat .value.seal { color: var(--accent); }

.form label { display: grid; gap: 6px; margin-bottom: 12px; }
.form input, .form select, .form textarea {
  background: rgba(255,255,255,0.06); border: 1px solid rgba(255,255,255,0.12);
  color: var(--text); border-radius: 10px; padding: 10px;
}
.form .actions { display: flex; gap: 8px; }

.list { margin: 12px 0; padding-left: 16px; }
.list li { margin: 6px 0; }
.fine { color: var(--muted); font-size: 0.9em; }

.logo-wrap { display: flex; justify-content: center; }
.ceremonial-frame { position: absolute; inset: 0; pointer-events: none; }
.frame { position: absolute; opacity: 0.8; width: 48px; height: 48px; }
.frame.butterfly { top: 10%; left: 6%; }
.frame.bee { top: 18%; right: 10%; }

@media (max-width: 640px) {
  .headline { font-size: 32px; }
  .content-grid { grid-template-columns: 1fr; }
}
